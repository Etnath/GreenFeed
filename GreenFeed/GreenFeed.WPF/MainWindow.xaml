<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GreenFeed.WPF"
        xmlns:view="clr-namespace:GreenFeed.WPF.View"
        xmlns:ViewModel="clr-namespace:GreenFeed.WPF.ViewModel" x:Class="GreenFeed.WPF.MainWindow"
        mc:Ignorable="d"
        Title="GreenFeed" Height="350" Width="525">
    <Window.DataContext>
        <ViewModel:MainWindowViewModel/>
    </Window.DataContext>
    <DockPanel>
        <view:RssFeedList x:Name="RssFeedList" DockPanel.Dock="Left"/>
        <view:RssPage x:Name="RssPage" />
    </DockPanel>
</Window>
