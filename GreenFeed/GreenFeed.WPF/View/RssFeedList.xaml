             mc:Ignorable="d" 
             d:DesignHeight="654" d:DesignWidth="300" MinWidth="300">

    <ListBox ItemsSource="{Binding RssFeedInfo, Mode=TwoWay}" MinWidth="300"/>
</UserControl>
